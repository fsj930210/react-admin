{
  "name": "vite-plugin-react-file-router",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.4.0",
  "description": "File system base react-router-dom v6 plugin for Vite",
  "license": "MIT",
  "keywords": [
    "vite",
    "react",
    "react-router-dom",
    "file-system-based",
    "routing"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "LICENSE",
    "client.d.ts",
    "dist"
  ],
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup"
  },
  "peerDependencies": {
    "vite": "^2.0.0 || ^3.0.0-0 || ^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "react-router-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "dequal": "^2.0.3",
    "extract-comments": "^1.1.0",
    "fast-glob": "^3.3.2",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@antfu/utils": "^0.7.10",
    "@types/glob-to-regexp": "^0.4.4",
    "@types/node": "^20.14.10",
    "react": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.3"
  }
}
