{
  "name": "react-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "一个react的中后台管理系统",
  "keywords": [
    "react",
    "antd5",
    "zustand",
    "react-router",
    "lightningcss",
    "vite",
    "react-admin",
    "ahooks",
    "esm"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{html,css}\" --cache-location node_modules/.cache/stylelint/",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "lint:fix": "lint-staged",
    "preview": "vite preview",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm"
  },
  "lint-staged": {
    "**/src/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ],
    "*.css": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.13.0",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "ahooks": "^3.8.0",
    "antd": "^5.19.1",
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.11",
    "lodash-es": "^4.17.21",
    "rc-slider-captcha": "^1.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-router-dom": "^6.24.1",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@antfu/utils": "^0.7.10",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.7.0",
    "@iconify-icons/ant-design": "^1.2.7",
    "@iconify-icons/carbon": "^1.2.20",
    "@iconify-icons/lucide": "^1.2.135",
    "@iconify-icons/ri": "^1.2.10",
    "@iconify/react": "^5.0.2",
    "@iconify/tools": "^4.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "browserslist": "^4.23.2",
    "eslint": "^9.7.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.8.0",
    "husky": "^9.1.1",
    "lightningcss": "^1.25.1",
    "lint-staged": "^15.2.7",
    "postcss-nesting": "^12.1.5",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "stylelint": "^16.7.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.16.1",
    "unocss": "^0.61.5",
    "vite": "^5.3.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svgr": "^4.2.0"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0",
    "pnpm": ">=9"
  }
}
