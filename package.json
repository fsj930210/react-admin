{
  "name": "react-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "一个react的中后台管理系统",
  "keywords": [
    "react",
    "antd5",
    "zustand",
    "react-router",
    "lightningcss",
    "vite",
    "react-admin",
    "ahooks",
    "esm"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{html,css}\" --cache-location node_modules/.cache/stylelint/",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "lint:fix": "lint-staged",
    "preview": "vite preview",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm"
  },
  "lint-staged": {
    "**/src/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ],
    "*.css": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "ahooks": "^3.8.1",
    "antd": "^5.21.6",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "i18next": "^23.16.4",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-chained-backend": "^4.6.2",
    "i18next-http-backend": "^2.6.2",
    "lodash-es": "^4.17.21",
    "rc-motion": "^2.9.3",
    "rc-resize-observer": "^1.4.0",
    "rc-slider-captcha": "^1.6.1",
    "rc-util": "^5.43.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.0",
    "react-router-dom": "^6.27.0",
    "uuid": "^10.0.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/compat": "^1.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@iconify-icons/ant-design": "^1.2.7",
    "@iconify-icons/carbon": "^1.2.20",
    "@iconify-icons/lucide": "^1.2.135",
    "@iconify-icons/ri": "^1.2.10",
    "@iconify/react": "^5.0.2",
    "@iconify/tools": "^4.0.7",
    "@remix-run/router": "^1.20.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "browserslist": "^4.24.2",
    "eslint": "^9.14.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "lightningcss": "^1.28.1",
    "lint-staged": "^15.2.10",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "unocss": "^0.63.6",
    "vite": "^5.4.10",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svgr": "^4.3.0"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0",
    "pnpm": ">=9"
  }
}
