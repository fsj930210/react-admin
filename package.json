{
  "name": "react-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "一个react的中后台管理系统",
  "keywords": [
    "react",
    "antd5",
    "zustand",
    "react-router",
    "lightningcss",
    "vite",
    "react-admin",
    "ahooks",
    "esm"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{html,css}\" --cache-location node_modules/.cache/stylelint/",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "lint:fix": "lint-staged",
    "preview": "vite preview",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm"
  },
  "lint-staged": {
    "**/src/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ],
    "*.css": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "ahooks": "^3.8.4",
    "antd": "^5.24.1",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.12.1",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.3",
    "i18next-chained-backend": "^4.6.2",
    "i18next-http-backend": "^3.0.2",
    "json-stable-stringify": "^1.3.0",
    "lodash-es": "^4.17.21",
    "lru-cache": "^11.1.0",
    "memoize-one": "^6.0.0",
    "mitt": "^3.0.1",
    "rc-motion": "^2.9.5",
    "rc-resize-observer": "^1.4.3",
    "rc-slider-captcha": "^1.7.2",
    "rc-util": "^5.44.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-router-dom": "^6.30.0",
    "react-virtualized-auto-sizer": "^1.0.25",
    "react-window": "^1.8.11",
    "uuid": "^11.0.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@iconify-json/ant-design": "^1.2.5",
    "@iconify-json/carbon": "^1.2.7",
    "@iconify-json/lucide": "^1.2.26",
    "@iconify-json/ri": "^1.2.5",
    "@iconify/react": "^5.2.0",
    "@iconify/tools": "^4.1.1",
    "@tailwindcss/vite": "^4.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.12",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "browserslist": "^4.24.4",
    "eslint": "^9.20.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lightningcss": "^1.29.1",
    "lint-staged": "^15.4.3",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.5.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svgr": "^4.3.0"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0",
    "pnpm": ">=9"
  }
}
